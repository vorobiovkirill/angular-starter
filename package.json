{
  "name": "angular-starter",
  "version": "1.0.0",
  "description": "Example angular project.",
  "author": "LehaMotovilov",
  "contributors": [
    {
      "name": "Aleksey Motovilov",
      "email": "lehaqs@gmail.com"
    }
  ],
  "license": "MIT",
  "homepage": "https://angular-starter.netlify.com/",
  "keywords": [
    "angular",
    "typescript"
  ],
  "engines": {
    "npm": ">4",
    "node": ">7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LehaMotovilov/angular-starter"
  },
  "bugs": "https://github.com/LehaMotovilov/angular-starter/issues",
  "scripts": {
    "build": "npm run build:local",
    "build:prod": "npm run rmrf && ENV=prod webpack --config configs/webpack.prod.js --hide-modules=true && rimraf dist/webpack-assets.json",
    "build:local": "npm run rmrf && ENV=dev webpack --config configs/webpack.local.js --hide-modules=true",
    "build:doc": "npm run compodoc && cp -R documentation dist",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "tslint \"src/**/*.ts\" --config ./tslint.json --project ./tsconfig.json --type-check --format codeFrame",
    "lint:css": "sass-lint --config ./.sass-lint.yml 'src/**/*.sass' --verbose",
    "clean-install": "npm cache clean --force && rimraf dist node_modules coverage && npm install && npm run build:prod",
    "start": "webpack-dev-server --config configs/webpack.local.js --watch",
    "watch": "npm run start",
    "test": "npm run rmrf && ENV=local karma start && npm run lint",
    "test:ci": "npm run rmrf && ENV=build karma start && npm run lint && codecov -f coverage/coverage.json",
    "e2e": "npm run webdriver:update && npm run build:prod && npm-run-all -p -r server:prod:ci protractor",
    "protractor": "protractor",
    "server:prod:ci": "superstatic dist --port 3000 --host 127.0.0.1 --config superstatic.json",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "compodoc": "./node_modules/.bin/compodoc -p ./tsconfig.json",
    "rmrf": "rimraf dist coverage documentation"
  },
  "browserslist": [
    "> 1%",
    "ie > 9"
  ],
